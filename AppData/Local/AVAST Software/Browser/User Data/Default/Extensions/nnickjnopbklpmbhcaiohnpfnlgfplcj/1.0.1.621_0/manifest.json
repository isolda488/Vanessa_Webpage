{
   "background": {
      "scripts": [ "js/background.js" ]
   },
   "browser_action": {
      "default_icon": "images/icon48.png",
      "default_popup": "html/popup.html",
      "default_title": "Browser Phishing Protection"
   },
   "content_scripts": [ {
      "all_frames": true,
      "css": [ "styles/content.css" ],
      "js": [ "js/content.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_idle"
   } ],
   "description": "This extension proactively detects Phishing webpages.",
   "icons": {
      "128": "images/icon128.png",
      "16": "images/icon16.png",
      "48": "images/icon48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Browser Phishing Protection",
   "permissions": [ "tabs", "webNavigation", "webRequest", "http://*/*", "https://*/*", "storage", "\u003Call_urls>", "activeTab", "system.memory" ],
   "version": "1.0.1.621",
   "web_accessible_resources": [ "images/*", "js/popup.js", "model/*", "database/dataset.json" ]
}
