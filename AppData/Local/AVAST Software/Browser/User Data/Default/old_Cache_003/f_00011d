			document.getElementById("var3409LabelImage").style.display = "none"
			break;
		default:
			document.getElementById("varietyName").innerHTML = "'Autumn Frost'";
			document.getElementById("varietyImage").src = "Content/plants/Hosta/ImgVariety/AutumnFrost.jpg";
			document.getElementById("var3404Pics").style.display = "block";
			document.getElementById("var3404Detail").style.display = "block";
			document.getElementById("var3404LabelImage").style.display = "none"
			break;
	}
}
function showBigVarPic(bigPicName) {
document.getElementById('varietyImage').src = 'Content/plants/Hosta/ImgVariety/' + bigPicName;
}
