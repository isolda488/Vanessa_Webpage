			document.getElementById("var3405Pics").style.display = "block";
			document.getElementById("var3405Detail").style.display = "block";
			document.getElementById("var3405LabelImage").style.display = "none"
			break;
		case 3406:
			document.getElementById("varietyName").innerHTML = "'Empress Wu'";
			document.getElementById("varietyImage").src = "Content/plants/Hosta/ImgVariety/Empresswu.jpg";
			document.getElementById("var3406Pics").style.display = "block";
			document.getElementById("var3406Detail").style.display = "block";
			document.getElementById("var3406LabelImage").style.display = "none"
			break;
