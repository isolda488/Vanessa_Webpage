function showVariety(id) {
	document.getElementById("var3404Pics").style.display = "none"
	document.getElementById("var3404Detail").style.display = "none"
	document.getElementById("var3405Pics").style.display = "none"
	document.getElementById("var3405Detail").style.display = "none"
	document.getElementById("var3406Pics").style.display = "none"
	document.getElementById("var3406Detail").style.display = "none"
	document.getElementById("var3407Pics").style.display = "none"
	document.getElementById("var3407Detail").style.display = "none"
	document.getElementById("var3408Pics").style.display = "none"
	document.getElementById("var3408Detail").style.display = "none"
	document.getElementById("var3409Pics").style.display = "none"
	document.getElementById("var3409Detail").style.display = "none"
	switch (id) {
		case 3404:
			document.getElementById("varietyName").innerHTML = "'Autumn Frost'";
			document.getElementById("varietyImage").src = "Content/plants/Hosta/ImgVariety/AutumnFrost.jpg";
			document.getElementById("var3404Pics").style.display = "block";
			document.getElementById("var3404Detail").style.display = "block";
			document.getElementById("var3404LabelImage").style.display = "none"
			break;
		case 3405:
			document.getElementById("varietyName").innerHTML = "'Coast To Coast'";
