	<script type="text/javascript">
		function showInfo(id) {
			var element;
			element = document.getElementById('TabDesc');
			element.className = 'LabelInactive';
			element = document.getElementById('TabUses');
			element.className = 'LabelInactive';
			element = document.getElementById('TabCare');
			element.className = 'LabelInactive';
			element = document.getElementById('TabStory');
			element.className = 'LabelInactive';
			element = document.getElementById('DescContent');
			element.style.display = "none";
			element = document.getElementById('UsesContent');
			element.style.display = "none";
			element = document.getElementById('CareContent');
			element.style.display = "none";
			element = document.getElementById('StoryContent');
			element.style.display = "none";
			switch (id) {
				case 1:
					element = document.getElementById('TabUses');
					element.className = 'LabelActive';
					element = document.getElementById('UsesContent');
					element.style.display = "block";
					break;
				case 2:
					element = document.getElementById('TabCare');
					element.className = 'LabelActive';
					element = document.getElementById('CareContent');
					element.style.display = "block";
					break;
				case 3:
					element = document.getElementById('TabStory');
					element.className = 'LabelActive';
					element = document.getElementById('StoryContent');
					element.style.display = "block";
					break;
				default:
					element = document.getElementById('TabDesc');
					element.className = 'LabelActive';
					element = document.getElementById('DescContent');
					element.style.display = "block";
					break;
			}
		}
	</script>
</head>
